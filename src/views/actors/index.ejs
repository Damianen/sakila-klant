<% /* receives items */ %>
<div class="mx-auto flex flex-col max-w-7xl items-center justify-between gap-x-6 p-6 lg:px-8">
<h1 class="text-center text-2xl/9 font-bold tracking-tight text-white">Actors</h1>
<ul>
  <% items.forEach(a => { %>
    <li class="flex gap-x-4 py-5">
    <img src="https://images.unsplash.com/photo-1494790108377-be9c29b29330?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80" alt="" class="size-12 flex-none rounded-full bg-gray-800 outline outline-1 -outline-offset-1 outline-white/10" />
    <div class="min-w-0 flex items-center">
      <a class="text-sm/6 font-semibold text-white" href="/actors/<%= a.actor_id %>"><%= a.first_name %> <%= a.last_name %></a>
    </div>
  </li>
  <% }) %>
</ul>
</div>

